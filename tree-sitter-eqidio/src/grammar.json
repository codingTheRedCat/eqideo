{
  "name": "eqidio",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "environment_command"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex"
        }
      ]
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_]*"
    },
    "natural": {
      "type": "PATTERN",
      "value": "\\+?[0-9]+"
    },
    "integer": {
      "type": "PATTERN",
      "value": "-[0-9]+"
    },
    "floating_point": {
      "type": "PATTERN",
      "value": "[-+]?[0-9]*(\\.?[0-9]+|([eE][-+]?[0-9]+)){1,2}"
    },
    "_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "natural"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "floating_point"
        }
      ]
    },
    "enum_value": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "@"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          }
        ]
      }
    },
    "duration": {
      "type": "PATTERN",
      "value": "(((\\d+m)|(\\d+s)|(\\d+ms))_?)+"
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "duration",
                      "content": {
                        "type": "SYMBOL",
                        "name": "duration"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "runtime",
                      "content": {
                        "type": "SYMBOL",
                        "name": "variable"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "argument",
            "content": {
              "type": "SYMBOL",
              "name": "_argument"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ";"
            },
            {
              "type": "SYMBOL",
              "name": "eol"
            }
          ]
        }
      ]
    },
    "environment_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "_"
        },
        {
          "type": "FIELD",
          "name": "id",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "argument",
            "content": {
              "type": "SYMBOL",
              "name": "_argument"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ";"
            },
            {
              "type": "SYMBOL",
              "name": "eol"
            }
          ]
        }
      ]
    },
    "eqtex": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$$"
        },
        {
          "type": "SYMBOL",
          "name": "_eqtex_expression"
        },
        {
          "type": "STRING",
          "value": "$$"
        }
      ]
    },
    "eqtex_char": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9]"
    },
    "eqtex_text": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9]+"
    },
    "_eqtex_script_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "eqtex_char"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "SYMBOL",
              "name": "eqtex_text"
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        }
      ]
    },
    "_eqtex_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex_sum"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex_product"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex_symbol"
        }
      ]
    },
    "_eqtex_factor": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex_symbol"
        }
      ]
    },
    "_eqtex_component": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex_product"
        },
        {
          "type": "SYMBOL",
          "name": "eqtex_symbol"
        }
      ]
    },
    "eqtex_sum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_eqtex_component"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "+"
              },
              {
                "type": "SYMBOL",
                "name": "_eqtex_component"
              }
            ]
          }
        }
      ]
    },
    "eqtex_product": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_eqtex_factor"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "*"
                },
                {
                  "type": "STRING",
                  "value": "\\times"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_eqtex_factor"
          }
        }
      ]
    },
    "eqtex_symbol": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "eqtex_text"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "_"
                },
                {
                  "type": "FIELD",
                  "name": "subscript",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_eqtex_script_content"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "^"
                },
                {
                  "type": "FIELD",
                  "name": "superscript",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_eqtex_script_content"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_argument": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "natural"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "floating_point"
        },
        {
          "type": "SYMBOL",
          "name": "enum_value"
        },
        {
          "type": "SYMBOL",
          "name": "duration"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "_newline"
    },
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "eol"
    },
    {
      "type": "SYMBOL",
      "name": "_newline"
    }
  ],
  "inline": [],
  "supertypes": []
}

