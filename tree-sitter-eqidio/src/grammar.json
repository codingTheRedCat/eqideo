{
  "name": "eqidio",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "environment_command"
        }
      ]
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "\\s+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_]*"
    },
    "natural": {
      "type": "PATTERN",
      "value": "\\+?[0-9]+"
    },
    "integer": {
      "type": "PATTERN",
      "value": "-[0-9]+"
    },
    "floating_point": {
      "type": "PATTERN",
      "value": "[-+]?[0-9]*(\\.?[0-9]+|([eE][-+]?[0-9]+)){1,2}"
    },
    "enum_value": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "@"
          },
          {
            "type": "SYMBOL",
            "name": "identifier"
          }
        ]
      }
    },
    "duration": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "minutes",
                  "content": {
                    "type": "SYMBOL",
                    "name": "natural"
                  }
                },
                {
                  "type": "STRING",
                  "value": "min"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "seconds",
                  "content": {
                    "type": "SYMBOL",
                    "name": "natural"
                  }
                },
                {
                  "type": "STRING",
                  "value": "s"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "milliseconds",
                  "content": {
                    "type": "SYMBOL",
                    "name": "natural"
                  }
                },
                {
                  "type": "STRING",
                  "value": "ms"
                }
              ]
            }
          ]
        }
      }
    },
    "variable": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "*"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "FIELD",
                "name": "declaration",
                "content": {
                  "type": "STRING",
                  "value": "="
                }
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          }
        ]
      }
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "time",
              "content": {
                "type": "SYMBOL",
                "name": "duration"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "controller",
              "content": {
                "type": "SYMBOL",
                "name": "variable"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "argument",
                  "content": {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_argument"
                    }
                  }
                },
                {
                  "type": "STRING",
                  "value": ";"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_argument": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "natural"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "floating_point"
        },
        {
          "type": "SYMBOL",
          "name": "enum_value"
        },
        {
          "type": "SYMBOL",
          "name": "duration"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "environment_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "_"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "argument",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_argument"
                  }
                },
                {
                  "type": "STRING",
                  "value": ";"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "inline": [],
  "supertypes": []
}

